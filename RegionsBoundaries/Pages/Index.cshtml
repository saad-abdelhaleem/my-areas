@page
@model IndexModel
@{
    ViewData["Title"] = "Pickup place";
}
@section Scripts{
    <script src="~/js/index.js" type="text/javascript"></script>
}
<div class="card shadow mb-4">
    <!-- Card Header - Dropdown -->
    <div class="card-header py-3 d-flex flex-row align-items-center justify-content-between">
        <h6 class="m-0 font-weight-bold text-primary d-none d-sm-inline-block">@ViewBag.Title</h6>
    </div>
    <!-- Card Body -->
    <div class="card-body">
        <div id="map" style="height:500px;"></div>
        <ul class="mt-3" id="loger" style="list-style: none;"></ul>
    </div>
</div>